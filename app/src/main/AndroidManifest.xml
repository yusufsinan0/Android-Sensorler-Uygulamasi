<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">



    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.19mart"
        tools:targetApi="31">


        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Accelerometer"></activity>
        <activity android:name=".Gyroscope"></activity>
        <activity android:name=".MagneticField"></activity>
        <activity android:name=".LightSensor"></activity>
        <activity android:name=".PressureSensor"></activity>
        <activity android:name=".TemperatureSensor"></activity>
        <activity android:name=".Proximity"></activity>
        <activity android:name=".GravitySensor"></activity>
        <activity android:name=".GpsSensor"></activity>
    </application>

</manifest>